{
  "name": "plaisio/blob-store-blob",
  "description": "PhpPlaisio: BLOB Store using BLOBs",
  "keywords": [
    "PhpPlaisio",
    "BLOB-Store"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/blob-store": "^1.3.0",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/kernel": "^3.2.1",
    "setbased/helper-program-execution": "^1.2.0",
    "setbased/php-stratum-mysql": "^7.0.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.0",
    "phpunit/phpunit": "^10.5.35",
    "plaisio/company-resolver-uni": "^2.1.0",
    "plaisio/console-kernel": "^1.0.0",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-backend": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\BlobStore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\BlobStore\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
