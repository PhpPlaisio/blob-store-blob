{
  "name": "plaisio/blob-store-blob",
  "description": "PhpPlaisio: BLOB Store using BLOBs",
  "keywords": [
    "PhpPlaisio",
    "BLOB-Store"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/blob-store": "^1.0",
    "plaisio/kernel": "^1.0",
    "setbased/helper-program-execution": "^1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/company-resolver-uni": "^1.0",
    "setbased/php-stratum": "^5.4",
    "setbased/php-stratum-mysql": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\BlobStore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\BlobStore\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
